{
  "name": "@minuk-hwang-design-system/style-tokens",
  "version": "0.0.1",
  "type": "module",
  "main": "index.js",
  "packageManager": "pnpm@8.15.0",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./style-tokens.css": "./dist/style-tokens.css"
  },
  "scripts": {
    "build": "pnpm build:js && pnpm build:type && pnpm build:css",
    "build:dev": "pnpm build:js --dev --watch & pnpm build:type --watch",
    "build:js": "node build.js",
    "build:type": "tsc --emitDeclarationOnly",
    "build:css": "node ./scripts/build-css-modules.js",
    "clean": "rimraf dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Minuk Hwang",
  "license": "MIT",
  "description": "Design system style tokens including colors, typography, spacing, and more",
  "homepage": "https://github.com/minukHwang/minuk-hwang-design-system/tree/main/packages/style-tokens#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minukHwang/minuk-hwang-design-system.git",
    "directory": "packages/style-tokens"
  },
  "bugs": {
    "url": "https://github.com/minukHwang/minuk-hwang-design-system/issues"
  },
  "keywords": [
    "design-system",
    "style-tokens",
    "design-tokens",
    "css-variables",
    "theme",
    "theming",
    "colors",
    "typography",
    "spacing",
    "tokens",
    "frontend",
    "ui",
    "minuk-hwang-design-system"
  ],
  "files": [
    "dist",
    "README.md",
    "LICENSE",
    "package.json"
  ],
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@minuk-hwang-design-system/esbuild-config": "workspace:^",
    "esbuild": "^0.23.0",
    "rimraf": "^6.0.1",
    "typescript": "^5.5.3"
  },
  "dependencies": {
    "material-symbols": "^0.21.2",
    "pretendard": "^1.3.9"
  }
}
